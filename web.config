<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <add name="httpPlatformHandler" path="*" verb="*" modules="httpPlatformHandler" resourceType="Unspecified" requireAccess="Script" />
        </handlers>
        <httpPlatform stdoutLogEnabled="true" stdoutLogFile=".\go.log" startupTimeLimit="20" processPath=".\configReciever.exe" arguments="--port %HTTP_PLATFORM_PORT%">
        </httpPlatform>
    </system.webServer>
</configuration>